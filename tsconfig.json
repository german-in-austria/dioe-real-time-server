{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": "src",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "lib": [
      "es2016",
      "dom",
      "dom.iterable"
    ]
  }
}
